{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "angular": true
  },
  "rules": {
    "no-var": 0,
    "comma-dangle": [2, "never"],
    "no-space-before-semi": 0,
    "space-after-function-name": 0,
    "semi-spacing": [2, { "before": false, "after": true }],
    "no-param-reassign": 0,
    "spaced-line-comment": 0
  }
}
